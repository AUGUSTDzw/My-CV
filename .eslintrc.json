{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"plugin:yml/standard"
	],
	"overrides": [
		{
			"files": ["*.yaml", "*.yml"],
			"parser": "yaml-eslint-parser"
		}
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint", "yaml"],
	"rules": {
        "@typescript-eslint/ban-ts-comment": "off"
    }
}
